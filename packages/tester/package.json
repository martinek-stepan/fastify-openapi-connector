{
	"name": "tester",
	"version": "0.1.0",
	"license": "MIT",
	"author": "Stepan Martinek",
	"type": "module",
	"main": "./src/index.ts",
	"scripts": {
		"debug": "tsx --watch src/index.ts",
		"generate:schema": "openapi-typescript --output ./src/generated/schema.ts ./openapi.yaml",
		"generate:handlers": "fastify-oas-generator --schema-file ./src/generated/schema.ts --types-file ./src/generated/types.ts --service-file ./src/generated/service.ts --spec-file ./openapi.yaml --override-types-file --paths-dir ./src/routes --security-dir ./src/securityHandlers --webhooks-dir ./src/webhooks"
	},
	"dependencies": {
		"fastify-openapi-connector-generator": "../generator",
		"@redocly/openapi-core": "^1.34.2",
		"fastify": "5",
		"fastify-openapi-connector": "../connector"
	},
	"devDependencies": {
		"openapi-typescript": "^7.10.1",
    	"typescript": "^5.9.3",
		"tsx": "^4.19.2",
		"@types/node": "22"
	}
}
